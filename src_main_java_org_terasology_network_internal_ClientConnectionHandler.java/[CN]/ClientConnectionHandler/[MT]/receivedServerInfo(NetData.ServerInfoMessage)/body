{
  logger.info("Received server info");
  ((EngineTime)CoreRegistry.get(Time.class)).setGameTime(message.getTime());
  this.server=new ServerImpl(networkSystem,channelHandlerContext.getChannel());
  server.setServerInfo(message);
  Config config=CoreRegistry.get(Config.class);
  channelHandlerContext.getChannel().write(NetData.NetMessage.newBuilder().setJoin(NetData.JoinMessage.newBuilder().setName(config.getPlayer().getName()).setViewDistanceLevel(config.getRendering().getViewDistance().getIndex())).build());
}
