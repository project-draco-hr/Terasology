{
  out.beginObject().name("node").value(treeGson.getId(value.getNode())).name("position").beginArray().value(value.getPosition().x).value(value.getPosition().y).endArray().name("size").beginArray().value(value.getSize().x).value(value.getSize().y).endArray().name("children");
  gsonNode.toJson(value.children(),List.class,out);
  out.endObject();
}
