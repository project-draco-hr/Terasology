{
  NetworkComponent netComponent=entity.getComponent(NetworkComponent.class);
  if (netComponent != null) {
    netIdToEntityId.remove(netComponent.networkId);
    if (mode == NetworkMode.SERVER) {
      NetData.NetMessage message=NetData.NetMessage.newBuilder().setType(NetData.NetMessage.Type.REMOVE_ENTITY).setRemoveEntity(NetData.RemoveEntityMessage.newBuilder().setNetId(netComponent.networkId).build()).build();
      for (      Client client : clientList) {
        client.setNetRemoved(netComponent.networkId,message);
      }
    }
  }
}
