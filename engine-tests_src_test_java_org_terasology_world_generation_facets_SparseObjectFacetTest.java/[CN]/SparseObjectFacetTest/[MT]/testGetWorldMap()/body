{
  facet.set(0,1,2,4);
  facet.set(0,1,3,3);
  facet.set(9,3,1,2);
  facet.setWorld(13,28,34,2);
  facet.setWorld(10,21,35,1);
  Map<Vector3i,Number> expected=ImmutableMap.of(new Vector3i(10,21,32),4,new Vector3i(10,21,33),3,new Vector3i(13,28,34),2,new Vector3i(10,21,35),1,new Vector3i(19,23,31),2);
  Assert.assertEquals(expected,facet.getWorldEntries());
}
