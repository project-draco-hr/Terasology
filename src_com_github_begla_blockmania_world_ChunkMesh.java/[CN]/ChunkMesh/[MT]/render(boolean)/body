{
  if (!translucent) {
    renderVbo(0);
  }
 else {
    glDisable(GL_CULL_FACE);
    glEnable(GL_BLEND);
    glBlendFunc(GL_SRC_ALPHA,GL_ONE_MINUS_SRC_ALPHA);
    renderVbo(1);
    glEnable(GL_BLEND);
    glDisable(GL_CULL_FACE);
    renderVbo(2);
    glEnable(GL_CULL_FACE);
    glDisable(GL_BLEND);
  }
}
