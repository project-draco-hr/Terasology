{
  if (_showTextured) {
    for (    String borderType : _images.keySet()) {
      _images.get(borderType).renderTransformed();
    }
    if (_showCorners) {
      for (      String cornerType : _corners.keySet()) {
        _corners.get(cornerType).renderTransformed();
      }
    }
  }
 else {
    renderSolid();
  }
}
