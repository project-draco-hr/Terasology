{
  File pluginDir=new File("groovy/plugins");
  File[] plugins=pluginDir.listFiles(new FileFilter(){
    public boolean accept(    File file){
      if (file.getName().contains(".groovy"))       return true;
      return false;
    }
  }
);
  for (  File p : plugins) {
    initializePlugin(p.getName());
  }
}
