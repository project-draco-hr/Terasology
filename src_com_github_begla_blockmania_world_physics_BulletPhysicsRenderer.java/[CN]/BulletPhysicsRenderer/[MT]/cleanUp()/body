{
  if (Blockmania.getInstance().getTime() - _lastCleanUp > 100) {
    if (_blocks.isEmpty())     return;
    BlockRigidBody b=_blocks.removeFirst();
    ;
    if (b.isActive()) {
      _blocks.add(b);
      return;
    }
    _discreteDynamicsWorld.removeRigidBody(b);
    _lastCleanUp=Blockmania.getInstance().getTime();
  }
}
