{
  _worldTimeEventManager.addWorldTimeEvent(new WorldTimeEvent(0.1,true){
    @Override public void run(){
      SoundStore.get().setMusicVolume(0.1f);
      AudioManager.getInstance().getAudio("Sunrise").playAsMusic(1.0f,1.0f,false);
    }
  }
);
