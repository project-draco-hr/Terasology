{
  _logger.log(Level.INFO,"Welcome to {0}!",Configuration.GAME_TITLE);
  Main main=null;
  try {
    main=new Main();
    main.create();
    main.start();
  }
 catch (  Exception ex) {
    _logger.log(Level.SEVERE,ex.toString(),ex);
  }
 finally {
    if (main != null) {
      main.destroy();
    }
  }
  System.exit(0);
}
