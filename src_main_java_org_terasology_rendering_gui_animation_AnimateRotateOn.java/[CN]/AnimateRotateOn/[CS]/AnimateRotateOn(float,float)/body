{
  this.angle=angle;
  this.speed=speed;
  this.id=generateId();
  if (angle < 0) {
    factor=-1;
  }
 else {
    factor=1;
  }
  Tessellator tessellator=new Tessellator();
  TessellatorHelper.addGUIQuadMesh(tessellator,new Vector4f(255f / 256f,255f / 256f,255f / 256f,1f),1.0f,1.0f);
  mesh=tessellator.generateMesh();
}
