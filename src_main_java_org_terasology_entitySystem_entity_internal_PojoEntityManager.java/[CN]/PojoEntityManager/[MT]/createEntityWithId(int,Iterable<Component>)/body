{
  if (!freedIds.contains(id)) {
    EntityRef entity=createEntityRef(id);
    for (    Component c : components) {
      store.put(id,c);
    }
    loadedIds.add(id);
    if (eventSystem != null) {
      eventSystem.send(entity,OnActivatedComponent.newInstance());
    }
    return entity;
  }
  return EntityRef.NULL;
}
