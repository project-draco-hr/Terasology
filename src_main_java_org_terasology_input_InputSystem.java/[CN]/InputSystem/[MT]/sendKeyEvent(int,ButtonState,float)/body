{
  KeyEvent event;
switch (state) {
case UP:
    event=KeyUpEvent.create(key,delta);
  break;
case DOWN:
event=KeyDownEvent.create(key,delta);
break;
case REPEAT:
event=KeyRepeatEvent.create(key,delta);
break;
default :
return false;
}
setupTarget(event);
for (EntityRef entity : getInputEntities()) {
entity.send(event);
if (event.isConsumed()) {
break;
}
}
return event.isConsumed();
}
