{
  final SortedSet<String> threads=new TreeSet<>();
  int line=0;
  for (  String thread : threads) {
    lines.get(line).setVisible(true);
    lines.get(line).setText(thread);
    line++;
    if (line >= lines.size()) {
      break;
    }
  }
  for (; line < lines.size(); line++) {
    lines.get(line).setVisible(false);
  }
}
