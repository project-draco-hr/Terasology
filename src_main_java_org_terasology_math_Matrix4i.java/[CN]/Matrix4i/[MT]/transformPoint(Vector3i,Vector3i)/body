{
  if (dest == null)   dest=new Vector3i();
  int x=val[m00] * vec.x + val[m01] * vec.y + val[m02] * vec.z + val[m03];
  int y=val[m10] * vec.x + val[m11] * vec.y + val[m12] * vec.z + val[m13];
  int z=val[m20] * vec.x + val[m21] * vec.y + val[m22] * vec.z + val[m23];
  dest.x=x;
  dest.y=y;
  dest.z=z;
  return dest;
}
