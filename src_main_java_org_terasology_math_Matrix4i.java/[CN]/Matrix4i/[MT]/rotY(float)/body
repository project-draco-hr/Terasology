{
  val[m00]=(int)Math.round(Math.cos(angle));
  val[m01]=0;
  val[m02]=(int)Math.round(Math.sin(angle));
  val[m03]=0;
  val[m10]=0;
  val[m11]=1;
  val[m12]=0;
  val[m13]=0;
  val[m20]=-(int)Math.round(Math.sin(angle));
  val[m21]=0;
  val[m22]=(int)Math.round(Math.cos(angle));
  val[m23]=0;
  val[m30]=0;
  val[m31]=0;
  val[m32]=0;
  val[m33]=1;
  return this;
}
