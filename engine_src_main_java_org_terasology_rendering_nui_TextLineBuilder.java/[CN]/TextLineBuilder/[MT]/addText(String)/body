{
  List<String> paragraphs=Arrays.asList(text.split("\\r?\\n"));
  for (  String paragraph : paragraphs) {
    String remainder=paragraph;
    while (!remainder.isEmpty()) {
      String[] split=remainder.split(" ",2);
      String word=split[0];
      if (split.length > 1) {
        remainder=split[1];
      }
 else {
        remainder="";
      }
      addWord(word);
    }
    endLine();
  }
}
