{
  int tmp=0;
  for (int i=0; i < repetitions; i++) {
    for (int y=0; y < array.getSizeY(); y++) {
      for (int z=0; z < array.getSizeZ(); z++) {
        for (int x=0; x < array.getSizeX(); x++) {
          tmp+=array.get(x,y,z);
          array.get(x,y,z);
        }
      }
    }
  }
  return tmp;
}
