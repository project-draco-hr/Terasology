{
  if (_lock.tryLock()) {
    try {
      if (!_disposed) {
        ChunkMesh[] oldNewMesh=_newMeshes;
        _newMeshes=newMesh;
        if (oldNewMesh != null) {
          for (          ChunkMesh element : oldNewMesh) {
            element.dispose();
          }
        }
      }
    }
  finally {
      _lock.unlock();
    }
  }
}
