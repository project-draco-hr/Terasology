{
  _lock.lock();
  try {
    if (_disposed) {
      return;
    }
    if (_activeMeshes != null)     for (    ChunkMesh _activeMeshe : _activeMeshes) {
      _activeMeshe.dispose();
    }
    if (_newMeshes != null) {
      for (      ChunkMesh _newMeshe : _newMeshes) {
        _newMeshe.dispose();
      }
    }
    _activeMeshes=null;
    _newMeshes=null;
    setDirty(true);
  }
  finally {
    _lock.unlock();
  }
}
