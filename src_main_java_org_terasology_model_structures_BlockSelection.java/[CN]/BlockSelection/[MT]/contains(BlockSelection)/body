{
  if (!possiblyOverlaps(otherSelection)) {
    return false;
  }
  for (  BlockPosition otherPos : otherSelection.positions()) {
    boolean contained=false;
    for (    BlockPosition myPos : _positions) {
      if (otherPos.equals(myPos)) {
        contained=true;
        break;
      }
    }
    if (!contained) {
      logger.debug("Position {} was not contained in this selection, returning false",otherPos);
      return false;
    }
  }
  return true;
}
