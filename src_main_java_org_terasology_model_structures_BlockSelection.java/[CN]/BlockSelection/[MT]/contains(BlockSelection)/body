{
  if (!possiblyOverlaps(otherSelection)) {
    return false;
  }
  for (  BlockPosition otherPos : otherSelection.positions()) {
    boolean contained=false;
    for (    BlockPosition myPos : _positions) {
      if (otherPos.equals(myPos)) {
        contained=true;
        break;
      }
    }
    if (!contained) {
      Terasology.getInstance().getLogger().log(Level.INFO,"Position " + otherPos + "was not contained in this selection, returning false");
      return false;
    }
  }
  return true;
}
