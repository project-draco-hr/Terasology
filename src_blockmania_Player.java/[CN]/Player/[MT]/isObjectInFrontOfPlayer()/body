{
  if (getParent() != null) {
    Vector2f direction=new Vector2f((float)accX,(float)accZ);
    try {
      direction.normalise();
    }
 catch (    Exception e) {
    }
    return getParent().isHitting(new Vector3f(getPosition().x + direction.x * 1.0f,getPosition().y - 1.0f,getPosition().z + direction.y * 1.0f));
  }
 else {
    return false;
  }
}
