{
  if (getParent() != null) {
    Vector2f direction=new Vector2f((float)accX,(float)accZ);
    try {
      direction.normalise();
    }
 catch (    Exception e) {
    }
    return getParent().isHitting(new Vector3f(getPosition().x + direction.x,getPosition().y - PLAYER_HEIGHT + 1,getPosition().z + direction.y));
  }
 else {
    return false;
  }
}
