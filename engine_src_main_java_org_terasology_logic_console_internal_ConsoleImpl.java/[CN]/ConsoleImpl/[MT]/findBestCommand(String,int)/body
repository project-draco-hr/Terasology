{
  TreeMultiset<ICommand> correspondingCommands=commandRegistry.get(name);
  if (correspondingCommands == null) {
    return null;
  }
  for (  ICommand command : correspondingCommands) {
    if ((command.getRequiredParameterCount() == args && !command.endsWithVarargs()) || (command.getRequiredParameterCount() <= args && command.endsWithVarargs())) {
      return command;
    }
  }
  return null;
}
