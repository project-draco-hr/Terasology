{
  String commandName=command.getName();
  logger.debug("Registering {}...",command.getName());
  if (commandRegistry.containsKey(commandName)) {
    logger.warn("Command with name '{}' already registered by class '{}', skipping '{}'",commandName,commandRegistry.get(commandName).getClass().getCanonicalName(),command.getClass().getCanonicalName());
  }
 else {
    commandRegistry.put(commandName,command);
    logger.info("Command '{}' successfully registered for class '{}'.",commandName,command.getClass().getCanonicalName());
  }
}
