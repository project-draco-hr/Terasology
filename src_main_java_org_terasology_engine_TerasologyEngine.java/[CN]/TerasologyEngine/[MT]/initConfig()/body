{
  if (Config.getConfigFile().exists()) {
    try {
      config=Config.load(Config.getConfigFile());
    }
 catch (    IOException e) {
      logger.error("Failed to load config",e);
      config=new Config();
    }
  }
 else {
    config=new Config();
  }
  config.getDefaultModSelection().addMod("core");
  CoreRegistry.put(Config.class,config);
}
