{
  try {
    Audio a=_audioFiles.get(s);
    if (a == null) {
      a=AudioLoader.getAudio("OGG",ResourceLoader.getResourceAsStream("org/terasology/data/sounds/" + s + ".ogg"));
      _audioFiles.put(s,a);
    }
    return a;
  }
 catch (  IOException e) {
    Terasology.getInstance().getLogger().log(Level.SEVERE,e.getLocalizedMessage());
  }
  return null;
}
