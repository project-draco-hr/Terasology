{
  PerformanceMonitor.startActivity("GenerateBlockSide");
  int vertexElementsId=0;
switch (renderType) {
case BILLBOARD_AND_TRANSLUCENT:
    vertexElementsId=1;
  break;
case WATER_AND_ICE:
vertexElementsId=3;
break;
}
if (meshPart != null) {
meshPart.appendTo(mesh,x,y,z,colorOffset,vertexElementsId);
}
PerformanceMonitor.endActivity();
}
