{
  super.apply(builder);
  if (inherit != null) {
    UISkin skin=Assets.getSkin(inherit);
    if (skin != null) {
      builder.setBaseSkin(skin);
    }
  }
  if (families != null) {
    for (    Map.Entry<String,FamilyInfo> entry : families.entrySet()) {
      builder.setFamily(entry.getKey());
      entry.getValue().apply(builder);
    }
  }
}
