{
  if (states.size() > 0) {
    if (stateID < 0) {
      stateID=0;
    }
    if (stateID >= states.size()) {
      stateID=states.size() - 1;
    }
    states.remove(stateID);
    if (states.size() == 0) {
      currentState=-1;
      getLabel().setText("");
    }
 else {
      nextState();
    }
  }
}
