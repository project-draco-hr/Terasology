{
  for (  Side side : Side.values()) {
    BlockPart part=BlockPart.fromSide(side);
    BlockMeshPart meshPart=shape.getMeshPart(part).rotate(Rotation.none().getQuat4f()).mapTexCoords(atlas.getTexCoords(tileUris.get(part),true),atlas.getRelativeTileSizeWithOffset());
    block.setLoweredLiquidMesh(part.getSide(),meshPart);
  }
}
