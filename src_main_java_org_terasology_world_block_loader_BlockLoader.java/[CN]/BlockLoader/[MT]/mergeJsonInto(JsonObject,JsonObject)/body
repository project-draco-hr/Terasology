{
  for (  Map.Entry<String,JsonElement> entry : from.entrySet()) {
    if (entry.getValue().isJsonObject()) {
      if (!to.has(entry.getKey())) {
        to.add(entry.getKey(),entry.getValue());
      }
    }
 else {
      if (!to.has(entry.getKey())) {
        to.add(entry.getKey(),entry.getValue());
      }
    }
  }
}
