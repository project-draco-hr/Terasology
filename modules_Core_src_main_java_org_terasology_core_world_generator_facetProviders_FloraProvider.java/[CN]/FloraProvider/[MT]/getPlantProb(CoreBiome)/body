{
switch (biome) {
case DESERT:
    return configuration.desertGrassDensity;
case FOREST:
  return configuration.forestGrassDensity;
case MOUNTAINS:
return configuration.mountainGrassDensity;
case PLAINS:
return configuration.plainsGrassDensity;
case SNOW:
return configuration.snowGrassDensity;
default :
return 0;
}
}
