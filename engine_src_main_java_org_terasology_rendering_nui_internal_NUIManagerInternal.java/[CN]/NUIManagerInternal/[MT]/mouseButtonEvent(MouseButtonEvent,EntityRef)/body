{
  if (focus != null) {
    focus.onMouseButtonEvent(event);
    if (event.isConsumed()) {
      return;
    }
  }
  if (event.isDown()) {
    if (canvas.processMouseClick(event.getButton(),Mouse.getPosition())) {
      event.consume();
    }
  }
 else {
    if (canvas.processMouseRelease(event.getButton(),Mouse.getPosition())) {
      event.consume();
    }
  }
}
