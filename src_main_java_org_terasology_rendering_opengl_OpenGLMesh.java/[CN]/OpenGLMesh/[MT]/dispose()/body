{
  hasTexCoord0=false;
  hasTexCoord1=false;
  hasColor=false;
  hasNormal=false;
  indexCount=0;
  if (vboVertexBuffer != 0) {
    VertexBufferObjectManager.getInstance().putVboId(vboVertexBuffer);
    vboVertexBuffer=0;
  }
  if (vboIndexBuffer != 0) {
    VertexBufferObjectManager.getInstance().putVboId(vboIndexBuffer);
    vboIndexBuffer=0;
  }
}
