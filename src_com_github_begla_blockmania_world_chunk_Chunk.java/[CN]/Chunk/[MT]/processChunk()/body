{
  generate();
  Chunk[] neighbors=loadOrCreateNeighbors();
  for (int i=0; i < neighbors.length; i++) {
    if (neighbors[i] != null) {
      neighbors[i].generate();
    }
  }
  if (isLightDirty()) {
    updateLight();
  }
  if (isDirty() && !isLightDirty() && !isFresh()) {
    generateMesh();
    return true;
  }
  return false;
}
