{
  if (_newMesh != null) {
    for (    Chunk nc : loadOrCreateNeighbors())     if ((nc.isDirty() || nc.isLightDirty()) && nc.isVisible())     return;
    if (_newMesh.isGenerated() && !isDirty() && !isFresh()&& !isLightDirty()) {
      ChunkMesh oldMesh=_activeMesh;
      _activeMesh=_newMesh;
      if (oldMesh != null)       oldMesh.freeBuffers();
      _newMesh=null;
    }
  }
}
