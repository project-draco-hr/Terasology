{
  if (o == null) {
    return 0;
  }
  if (getParent().getPlayer() != null) {
    double distance=distanceToPlayer();
    double distance2=o.distanceToPlayer();
    if (distance == distance2)     return 0;
    return distance2 > distance ? -1 : 1;
  }
  return getChunkId().compareTo(o.getChunkId());
}
