{
synchronized (this) {
    if (_disposed)     return false;
    if (_newMesh != null) {
      if (_newMesh.isDisposed() || !_newMesh.isGenerated())       return false;
      ChunkMesh newMesh=_newMesh;
      _newMesh=null;
      ChunkMesh oldActiveMesh=_activeMesh;
      _activeMesh=newMesh;
      if (oldActiveMesh != null)       oldActiveMesh.dispose();
      return true;
    }
  }
  return false;
}
