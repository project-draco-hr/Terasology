{
  getPosition().setX(in.readInt());
  getPosition().setZ(in.readInt());
  byte flags=in.readByte();
  _lightDirty=Helper.isFlagSet(flags,(short)0);
  for (int i=0; i < _blocks.getSize(); i++)   _blocks.setRawByte(i,in.readByte());
  for (int i=0; i < _sunlight.getPackedSize(); i++)   _sunlight.setRawByte(i,in.readByte());
  for (int i=0; i < _light.getPackedSize(); i++)   _light.setRawByte(i,in.readByte());
  _fresh=false;
  _dirty=true;
}
