{
  final List<Side> result=Lists.newArrayList();
  sideBits.forEachEntry(new TObjectByteProcedure<Side>(){
    @Override public boolean execute(    Side a,    byte b){
      if ((b & sidesBit) > 0) {
        result.add(a);
      }
      return true;
    }
  }
);
  return Sets.newEnumSet(result,Side.class);
}
