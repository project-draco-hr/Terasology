{
  EntityData.Value.Builder result=EntityData.Value.newBuilder();
  for (  EntityRef ref : value) {
    if (!ref.exists()) {
      result.addInteger(0);
    }
 else {
      if (refInterceptor.get() != null) {
        refInterceptor.get().savingRef(ref.getId());
      }
      result.addInteger((ref).getId());
    }
  }
  return result.build();
}
