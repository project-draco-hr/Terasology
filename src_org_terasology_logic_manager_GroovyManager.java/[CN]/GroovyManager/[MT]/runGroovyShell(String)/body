{
  Terasology.getInstance().getLogger().log(Level.INFO,"Groovy console about to execute command: " + consoleString);
  updateBinding();
  GroovyShell shell=new GroovyShell(_bind);
  try {
    shell.evaluate(consoleString);
    return true;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return false;
  }
}
