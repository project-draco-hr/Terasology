{
  worldSeed=seed;
  world=new WorldBuilder(worldSeed.hashCode()).addProvider(new SeaLevelProvider()).addProvider(new HeightMapSurfaceHeightProvider()).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new FloraProvider()).addProvider(new TreeProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).build();
}
