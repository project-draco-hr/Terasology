{
  return new WorldBuilder(seed).addProvider(new SeaLevelProvider()).addProvider(new HeightMapSurfaceHeightProvider()).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new DefaultFloraProvider()).addProvider(new DefaultTreeProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).addPlugins();
}
