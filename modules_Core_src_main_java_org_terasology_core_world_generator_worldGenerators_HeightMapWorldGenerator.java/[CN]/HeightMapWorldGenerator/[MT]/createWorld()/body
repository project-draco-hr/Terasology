{
  return new WorldBuilder(CoreRegistry.get(Context.class)).setSeaLevel(16).addProvider(new SeaLevelProvider(16)).addProvider(new HeightMapSurfaceHeightProvider()).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new DefaultFloraProvider()).addProvider(new DefaultTreeProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).addPlugins();
}
