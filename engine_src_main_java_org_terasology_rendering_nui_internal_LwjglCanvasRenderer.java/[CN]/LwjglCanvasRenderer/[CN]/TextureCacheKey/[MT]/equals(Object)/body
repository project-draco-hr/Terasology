{
  if (this == obj) {
    return true;
  }
  if (obj instanceof TextureCacheKey) {
    TextureCacheKey other=(TextureCacheKey)obj;
    return Objects.equals(textureSize,other.textureSize) && Objects.equals(areaSize,other.areaSize) && Objects.equals(border,other.border)&& tiled == other.tiled;
  }
  return false;
}
