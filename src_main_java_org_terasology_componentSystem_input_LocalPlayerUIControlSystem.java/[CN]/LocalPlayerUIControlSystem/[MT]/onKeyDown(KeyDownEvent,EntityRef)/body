{
switch (event.getKey()) {
case Keyboard.KEY_F3:
    Config.getInstance().setDebug(!Config.getInstance().isDebug());
  break;
case Keyboard.KEY_F:
toggleViewingDistance();
break;
case Keyboard.KEY_F12:
CoreRegistry.get(WorldRenderer.class).printScreen();
break;
case Keyboard.KEY_F4:
metrics.toggleMode();
break;
}
}
