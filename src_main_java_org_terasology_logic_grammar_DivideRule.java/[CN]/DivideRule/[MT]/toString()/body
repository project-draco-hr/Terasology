{
  StringBuilder builder=new StringBuilder();
  builder.append("divide ");
  builder.append(direction.name());
  builder.append(" { \n");
  for (  DivideArg arg : args) {
    builder.append("\t ");
    builder.append(arg.toString());
    builder.append("\n");
  }
  builder.append("};");
  return builder.toString();
}
