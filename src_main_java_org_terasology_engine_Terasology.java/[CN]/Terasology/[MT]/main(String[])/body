{
  try {
    PathManager.getInstance().determineRootPath(true);
    TerasologyEngine engine=new TerasologyEngine();
    engine.init();
    engine.run(new StateMainMenu());
    engine.dispose();
  }
 catch (  Throwable t) {
    LoggerFactory.getLogger(Terasology.class).error("Uncaught Exception",t);
  }
  System.exit(0);
}
