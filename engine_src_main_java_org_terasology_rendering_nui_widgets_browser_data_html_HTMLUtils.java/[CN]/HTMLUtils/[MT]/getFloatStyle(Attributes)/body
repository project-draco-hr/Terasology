{
  String floatStr=getStyleAttribute(attributes,"float");
  if (floatStr == null) {
    return null;
  }
switch (floatStr) {
case "left":
    return ParagraphRenderStyle.FloatStyle.LEFT;
case "right":
  return ParagraphRenderStyle.FloatStyle.RIGHT;
case "none":
return ParagraphRenderStyle.FloatStyle.NONE;
}
return null;
}
