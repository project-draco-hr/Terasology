{
  if (this == obj) {
    return true;
  }
  if (obj instanceof TextCacheKey) {
    TextCacheKey other=(TextCacheKey)obj;
    return Objects.equals(text,other.text) && Objects.equals(font,other.font) && Objects.equals(color,other.color)&& Objects.equals(shadowColor,other.shadowColor)&& Objects.equals(width,other.width);
  }
  return false;
}
