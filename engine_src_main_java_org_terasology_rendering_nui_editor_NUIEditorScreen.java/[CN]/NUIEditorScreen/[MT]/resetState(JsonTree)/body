{
  getEditor().setTreeViewModel(node,true);
  resetPreviewWidget();
  getEditor().clearHistory();
  if (selectedAsset != null) {
    Optional<UIElement> asset=assetManager.getAsset(selectedAsset,UIElement.class);
    if (asset.isPresent()) {
      asset.get().dispose();
    }
  }
  updateConfig();
}
