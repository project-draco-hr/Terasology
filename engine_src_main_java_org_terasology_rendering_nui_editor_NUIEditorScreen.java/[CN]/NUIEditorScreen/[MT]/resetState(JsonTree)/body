{
  setTreeViewModel(node,true);
  setPreviewWidget(node);
  history.clear();
  history.add(node);
  historyPosition=0;
  if (selectedAsset != null) {
    Optional<UIElement> asset=assetManager.getAsset(selectedAsset,UIElement.class);
    if (asset.isPresent()) {
      asset.get().dispose();
    }
  }
  updateConfig();
}
