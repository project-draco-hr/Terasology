{
  labelSize=new Vector2i();
  for (  Property<?,?> property : propertyProvider.getProperties()) {
    Vector2i size=canvas.calculateSize(property.getLabel(),areaHint);
    if (size.x > labelSize.x) {
      labelSize.x=size.x;
    }
    if (size.y > labelSize.y) {
      labelSize.y=size.y;
    }
  }
}
