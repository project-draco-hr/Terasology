{
  Prefab prefab=entityManager.getPrefabManager().getPrefab(prefabName);
  int destroyed=0;
  if (prefab != null) {
    for (    EntityRef entity : entityManager.getAllEntities()) {
      if (prefab.getURI().equals(entity.getPrefabURI())) {
        entity.destroy();
        destroyed++;
      }
    }
  }
  return "Destroyed " + destroyed + " entities.";
}
