{
  UIStyle style=getCurrentStyle();
  if (!style.getMargin().isEmpty()) {
    return Rect2i.createFromMinAndMax(region.minX() + style.getMargin().getLeft(),region.minY() + style.getMargin().getTop(),region.maxX() - style.getMargin().getRight(),region.maxY() - style.getMargin().getBottom());
  }
  return region;
}
