{
  List<UITabItem> items=getItems();
  if (items.contains(item)) {
    if (item != selection) {
      ((StackLayout)tabs.getLayout()).setTop(item);
      if (selection != null) {
        selection.getTab().select(false);
      }
      selection=item;
      selection.getTab().select(true);
      notifySelectionListeners();
    }
  }
}
