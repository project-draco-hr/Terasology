{
  return new GsonBuilder().registerTypeAdapter(Name.class,new NameTypeAdapter()).registerTypeAdapter(Version.class,new VersionTypeAdapter()).registerTypeAdapter(BindsConfig.class,new BindsConfig.Handler()).registerTypeAdapter(SetMultimap.class,new SetMultimapTypeAdapter<>(Input.class)).registerTypeAdapter(SecurityConfig.class,new SecurityConfig.Handler()).registerTypeAdapter(Input.class,new InputHandler()).registerTypeAdapter(PixelFormat.class,new PixelFormatHandler()).registerTypeAdapterFactory(new CaseInsensitiveEnumTypeAdapterFactory()).registerTypeAdapterFactory(new UriTypeAdapterFactory()).setPrettyPrinting().create();
}
