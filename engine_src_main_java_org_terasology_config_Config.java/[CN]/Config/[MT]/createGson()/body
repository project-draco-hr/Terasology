{
  return new GsonBuilder().registerTypeAdapter(BindsConfig.class,new BindsConfig.Handler()).registerTypeAdapter(Multimap.class,new MultimapTypeAdapter<>(Input.class)).registerTypeAdapter(SecurityConfig.class,new SecurityConfig.Handler()).registerTypeAdapter(Input.class,new InputHandler()).registerTypeAdapter(PixelFormat.class,new PixelFormatHandler()).registerTypeAdapterFactory(new CaseInsensitiveEnumTypeAdapterFactory()).registerTypeAdapterFactory(new UriTypeAdapterFactory()).setPrettyPrinting().create();
}
