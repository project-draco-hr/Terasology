{
  List<Parameter> result=Lists.newArrayList();
  Class<?> cls=obj.getClass();
  for (  Field fld : cls.getFields()) {
    Parameter p;
    p=matchString(fld,obj);
    if (p != null) {
      result.add(p);
    }
    p=matchInt(fld,obj);
    if (p != null) {
      result.add(p);
    }
  }
  return result;
}
