{
  int chunkUpdates=0;
  List<Chunk> deletedElements=new ArrayList<Chunk>();
  for (  Chunk c : chunkUpdateQueue) {
    if (chunkUpdates < 16) {
      if (!c.dirty) {
        c.generateDisplayList();
        deletedElements.add(c);
        chunkUpdates++;
      }
    }
 else {
      break;
    }
  }
  for (  Chunk c : deletedElements) {
    removeChunkFromQueue(c);
  }
}
