{
  this.reflections=reflections;
  this.info=moduleInfo;
  this.version=Version.create(moduleInfo.getVersion());
  this.source=new ClasspathSource(id,getClass().getProtectionDomain().getCodeSource(),TerasologyConstants.ASSETS_SUBDIRECTORY,TerasologyConstants.OVERRIDES_SUBDIRECTORY,TerasologyConstants.DELTAS_SUBDIRECTORY);
}
