{
  CircularToggleSet<Character> set=new CircularToggleSet<>();
  assertNull(set.toggle());
  assertEquals(0,set.size());
  assertTrue(set.add('B'));
  assertEquals(1,set.size());
  assertEquals((Character)'B',set.toggle());
  assertEquals((Character)'B',set.toggle());
  assertTrue(set.remove('B'));
  assertNull(set.toggle());
  assertTrue(set.add('B'));
  assertEquals((Character)'B',set.toggle());
  assertEquals((Character)'B',set.toggle());
}
