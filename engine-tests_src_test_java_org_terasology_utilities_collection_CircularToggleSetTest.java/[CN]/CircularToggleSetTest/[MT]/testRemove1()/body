{
  CircularToggleSet<Character> set=new CircularToggleSet<>();
  String abcd="ABCD";
  for (int i=0; i < abcd.length(); i++) {
    Character c=abcd.charAt(i);
    assertTrue(set.add(c));
  }
  set.toggle();
  assertEquals((Character)'B',set.toggle());
  set.remove('C');
  assertEquals((Character)'D',set.toggle());
}
