{
  CircularToggleSet<Character> set=new CircularToggleSet<>();
  String abcd="ABCD";
  for (int i=0; i < abcd.length(); i++) {
    Character c=abcd.charAt(i);
    set.add(c);
  }
  for (int i=0; i < abcd.length(); i++) {
    Character c=abcd.charAt(i);
    assertEquals(c,set.toggle());
  }
  for (int i=0; i < abcd.length(); i++) {
    Character c=abcd.charAt(i);
    assertEquals(c,set.toggle());
  }
}
