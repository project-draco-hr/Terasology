def draw(self, context):
    layout = self.layout
    scene = context.scene
    if (not scene):
        return
    layout.prop(scene, 'teraAuthor')
    layout.prop(scene, 'teraAutoCollider')
    ob = context.object
    if (not ob):
        return
    if (not (ob.type == 'MESH')):
        return
    layout.prop(ob, 'teraFullSide')
    layout.prop(ob, 'teraAABB')
