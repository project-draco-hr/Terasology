{
  TLongList items=new TLongArrayList();
  for (  EntityRef ref : value) {
    if (!ref.exists()) {
      items.add(0L);
    }
 else {
      if (refInterceptor.get() == null || refInterceptor.get().savingRef(ref)) {
        items.add((ref).getId());
      }
 else {
        items.add(0L);
      }
    }
  }
  return context.create(items.iterator());
}
