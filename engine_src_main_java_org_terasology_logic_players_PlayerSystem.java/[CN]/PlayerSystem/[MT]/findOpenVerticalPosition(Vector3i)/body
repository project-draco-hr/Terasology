{
  int consecutiveAirBlocks=0;
  for (int i=1; i < 20; i++) {
    spawnPos.add(0,1,0);
    if (worldProvider.getBlock(spawnPos) == BlockManager.getAir()) {
      consecutiveAirBlocks++;
    }
 else {
      consecutiveAirBlocks=0;
    }
    if (consecutiveAirBlocks >= 2) {
      spawnPos.add(0,1 - consecutiveAirBlocks,0);
      return true;
    }
  }
  return false;
}
