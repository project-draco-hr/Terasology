{
  gameThread=new Thread(){
    @Override public void run(){
      try {
        PathManager.getInstance().useDefaultHomePath();
        engine=new TerasologyEngine();
        engine.run(new StateMainMenu());
        engine.dispose();
      }
 catch (      Exception e) {
        logger.error(e.toString(),e);
      }
    }
  }
;
  gameThread.start();
}
