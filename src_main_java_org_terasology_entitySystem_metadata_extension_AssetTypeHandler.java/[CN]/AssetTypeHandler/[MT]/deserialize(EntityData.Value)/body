{
  if (value.getStringCount() > 0) {
    AssetUri uri=new AssetUri(type,value.getString(0));
    if (uri.isValid()) {
      Asset asset=AssetManager.load(uri);
      if (asset != null && assetClass.isAssignableFrom(asset.getClass())) {
        return assetClass.cast(asset);
      }
    }
  }
  return null;
}
