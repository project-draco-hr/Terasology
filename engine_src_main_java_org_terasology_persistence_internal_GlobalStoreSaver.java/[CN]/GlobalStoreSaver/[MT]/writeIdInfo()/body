{
  store.setNextEntityId(entityManager.getNextId());
  entityManager.getFreedIds().forEach(new TIntProcedure(){
    public boolean execute(    int i){
      store.addFreedEntityId(i);
      return true;
    }
  }
);
  nonPersistentIds.forEach(new TIntProcedure(){
    public boolean execute(    int i){
      store.addFreedEntityId(i);
      return true;
    }
  }
);
}
