{
  EngineEntityManager entityManager=(EngineEntityManager)CoreRegistry.get(EntityManager.class);
  StorageManager storageManager=CoreRegistry.put(StorageManager.class,new StorageManagerInternal(CoreRegistry.get(ModuleManager.class),entityManager));
  try {
    storageManager.loadGlobalStore();
  }
 catch (  IOException e) {
    logger.error("Failed to load global data.",e);
  }
  return true;
}
