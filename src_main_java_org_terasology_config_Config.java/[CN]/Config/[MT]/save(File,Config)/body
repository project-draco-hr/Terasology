{
  FileWriter writer=new FileWriter(toFile);
  try {
    new GsonBuilder().registerTypeAdapter(BindsConfig.class,new BindsConfig.Handler()).registerTypeAdapter(Multimap.class,new MultimapHandler<Input>(Input.class)).registerTypeAdapter(Input.class,new InputHandler()).registerTypeAdapter(AdvancedConfig.class,new AdvancedConfig.Handler()).registerTypeAdapter(PixelFormat.class,new PixelFormatHandler()).setPrettyPrinting().create().toJson(config,writer);
  }
  finally {
    writer.close();
  }
}
