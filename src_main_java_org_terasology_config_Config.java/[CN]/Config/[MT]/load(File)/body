{
  FileReader reader=new FileReader(fromFile);
  try {
    return new GsonBuilder().registerTypeAdapter(InputConfig.class,new InputConfig.Handler()).registerTypeAdapter(Multimap.class,new MultimapHandler<Input>(Input.class)).registerTypeAdapter(Input.class,new InputHandler()).create().fromJson(reader,Config.class);
  }
  finally {
    reader.close();
  }
}
