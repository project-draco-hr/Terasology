{
  int height=_rand.nextInt() % 2 + 6;
  for (int i=0; i < height; i++) {
    setBlock(posX,posY + i,posZ,0x5,true);
  }
  for (int y=height - 2; y < height + 2; y+=1) {
    for (int x=-2; x < 3; x++) {
      for (int z=-2; z < 3; z++) {
        setBlock(posX + x,posY + y,posZ + z,0x6,true);
      }
    }
  }
}
