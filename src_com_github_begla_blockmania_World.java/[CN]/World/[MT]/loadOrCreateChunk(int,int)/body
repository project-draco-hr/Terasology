{
  Chunk c=_chunkCache.get(Helper.getInstance().cantorize(x,z));
  if (c != null) {
    return c;
  }
  if (_chunkCache.size() == 1024) {
    for (int i=0; i < 16; i++) {
      _chunkCache.pollFirstEntry();
    }
  }
  c=new Chunk(this,new Vector3f(x,0,z));
  _chunkCache.put(Helper.getInstance().cantorize(x,z),c);
  return c;
}
