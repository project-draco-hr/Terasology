{
  int height=_rand.nextInt() % 4 + 12;
  for (int i=0; i < height; i++) {
    setBlock(posX,posY + i,posZ,0x5);
  }
  for (int y=0; y < 10; y+=2) {
    for (int x=-5 + y / 2; x <= 5 - y / 2; x++) {
      for (int z=-5 + y / 2; z <= 5 - y / 2; z++) {
        if (!(x == 0 && z == 0)) {
          setBlock(posX + x,posY + y + (height - 10),posZ + z,0x6);
        }
      }
    }
  }
}
