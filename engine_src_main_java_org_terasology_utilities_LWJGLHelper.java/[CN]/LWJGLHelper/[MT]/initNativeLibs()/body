{
  initLibraryPaths();
  try {
    initOpenAL();
  }
 catch (  UnsatisfiedLinkError e) {
    logger.warn("Could not load OpenAL native libraries - sound disabled",e);
  }
  try {
    initOculus();
  }
 catch (  UnsatisfiedLinkError e) {
    logger.warn("Could not load optional TeraOVR native libraries - Oculus support disabled");
  }
}
