{
  logger.debug("Disposing shader {}.",getURI());
  TIntIntIterator it=fragmentPrograms.iterator();
  while (it.hasNext()) {
    it.advance();
    GL20.glDeleteShader(it.value());
  }
  fragmentPrograms.clear();
  it=vertexPrograms.iterator();
  while (it.hasNext()) {
    it.advance();
    GL20.glDeleteShader(it.value());
  }
  vertexPrograms.clear();
  shaderProgramBase=null;
}
