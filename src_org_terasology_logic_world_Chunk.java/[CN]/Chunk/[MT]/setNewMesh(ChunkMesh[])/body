{
  if (_lock.tryLock()) {
    try {
      if (!_disposed) {
        ChunkMesh[] oldNewMesh=_newMeshes;
        _newMeshes=newMesh;
        if (oldNewMesh != null) {
          for (int i=0; i < oldNewMesh.length; i++)           oldNewMesh[i].dispose();
        }
      }
    }
  finally {
      _lock.unlock();
    }
  }
}
