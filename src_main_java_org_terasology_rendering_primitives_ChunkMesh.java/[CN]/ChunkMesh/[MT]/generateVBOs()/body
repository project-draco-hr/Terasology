{
  if (lock.tryLock()) {
    try {
      if (vertexElements == null || disposed) {
        return false;
      }
      for (int i=0; i < vertexBuffers.length; i++) {
        generateVBO(i);
      }
      vertexElements=null;
      triangleCount=(vertexCount[0] + vertexCount[1] + vertexCount[2]+ vertexCount[3]) / 3;
    }
  finally {
      lock.unlock();
    }
    return true;
  }
  return false;
}
