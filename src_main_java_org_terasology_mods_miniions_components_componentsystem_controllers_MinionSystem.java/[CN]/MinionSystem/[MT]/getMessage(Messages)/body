{
switch (messages) {
case NoPath:
{
      String[] content=new String[3];
      content[0]="Command not executed";
      content[1]="No Path found";
      content[2]="The minion couldn't find a path to destination";
      return content;
    }
case Idle:
{
    String[] content=new String[3];
    content[0]="No more commands";
    content[1]="iddle minion";
    content[2]="The minion has no more commands to execute and is now iddle";
    return content;
  }
}
return null;
}
