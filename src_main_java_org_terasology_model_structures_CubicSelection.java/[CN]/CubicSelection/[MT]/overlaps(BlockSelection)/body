{
  int maxX=calcMaxX();
  int minX=calcMinX();
  int maxY=calcMaxY();
  int minY=calcMinY();
  int maxZ=calcMaxZ();
  int minZ=calcMinZ();
  for (  BlockPosition targetPos : otherSelection.positions()) {
    if (targetPos.x <= maxX && targetPos.x >= minX && targetPos.y <= maxY && targetPos.y >= minY && targetPos.z <= maxZ && targetPos.z >= minZ) {
      logger.trace("Selections overlap at {}, maybe more, returning true",targetPos);
      return true;
    }
  }
  return false;
}
