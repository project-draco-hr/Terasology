{
  if (!disposed) {
    disposed=true;
    LwjglSubRegion region=subregionStack.pop();
    while (!region.equals(this)) {
      logger.error("UI SubRegions being closed in an incorrect order");
      region.close();
      region=subregionStack.pop();
    }
    if (croppingRegion) {
      crop(previousState.cropRegion);
    }
    state=previousState;
  }
}
