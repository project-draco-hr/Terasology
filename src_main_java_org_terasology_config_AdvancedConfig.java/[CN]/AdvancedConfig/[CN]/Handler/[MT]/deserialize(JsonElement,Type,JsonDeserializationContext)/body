{
  AdvancedConfig config=AdvancedConfig.createDefault();
  JsonObject input=json.getAsJsonObject();
  config.setBlocksFactoryDontThrow(input.get("blocksFactory").getAsString()).setSunlightFactoryDontThrow(input.get("sunlightFactory").getAsString()).setLightFactoryDontThrow(input.get("lightFactory").getAsString()).setLiquidFactoryDontThrow(input.get("liquidFactory").getAsString()).setChunkDeflationEnabled(input.get("chunkDeflationEnabled").getAsBoolean()).setChunkDeflationLoggingEnabled(input.get("chunkDeflationLoggingEnabled").getAsBoolean());
  return config;
}
