{
  int stateToRemove=stateId;
  if (states.size() > 0) {
    if (stateToRemove < 0) {
      stateToRemove=0;
    }
    if (stateToRemove >= states.size()) {
      stateToRemove=states.size() - 1;
    }
    states.remove(stateToRemove);
    if (states.size() == 0) {
      currentState=-1;
      getLabel().setText("");
    }
 else {
      nextState();
    }
  }
}
