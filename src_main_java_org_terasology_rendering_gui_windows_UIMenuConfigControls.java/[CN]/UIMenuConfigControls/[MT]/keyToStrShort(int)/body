{
  if (key < 256) {
    return Keyboard.getKeyName(key);
  }
 else {
    if (key == 256) {
      return "ML";
    }
 else     if (key == 257) {
      return "MR";
    }
 else     if (key == 258) {
      return "MM";
    }
 else     if (key == 259) {
      return "UP";
    }
 else     if (key == 260) {
      return "DOWN";
    }
 else {
      return "MOVE";
    }
  }
}
