{
  if (editButton != null) {
    int key=-1;
    if (button == 0)     key=256;
 else     if (button == 1)     key=257;
 else     if (button == 2)     key=258;
 else     if (wheelMoved > 0)     key=259;
 else     if (wheelMoved < 0)     key=260;
    if (key != -1) {
      changeButton(editButton,key);
      editButton=null;
    }
  }
  consumed=super.processMouseInput(button,state,wheelMoved,consumed);
  return consumed;
}
