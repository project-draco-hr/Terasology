{
  String[] split=uri.toLowerCase(Locale.ENGLISH).split(PACKAGE_SEPARATOR,2);
  if (split.length > 1) {
    packageName=split[0];
    split=split[1].split(IDENTIFIER_SEPARATOR_REGEX,2);
    if (split.length > 1) {
      familyName=split[0];
      blockIdentifier=split[1];
    }
 else     if (split.length == 1) {
      familyName=split[0];
    }
  }
}
