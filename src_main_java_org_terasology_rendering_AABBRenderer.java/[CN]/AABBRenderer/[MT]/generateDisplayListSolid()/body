{
  displayListSolid=glGenLists(1);
  glNewList(displayListSolid,GL11.GL_COMPILE);
  glBegin(GL_QUADS);
  glColor4f(1.0f,1.0f,1.0f,1.0f);
  Vector3f dimensions=aabb.getExtents();
  GL11.glVertex3f(-dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,dimensions.z);
  GL11.glVertex3f(-dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(-dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(-dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(dimensions.x,dimensions.y,-dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,-dimensions.z);
  GL11.glVertex3f(dimensions.x,-dimensions.y,dimensions.z);
  GL11.glVertex3f(-dimensions.x,-dimensions.y,dimensions.z);
  glEnd();
  glEndList();
}
