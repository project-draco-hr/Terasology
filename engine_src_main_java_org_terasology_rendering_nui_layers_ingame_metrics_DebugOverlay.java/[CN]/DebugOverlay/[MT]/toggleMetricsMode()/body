{
  if (!metricsModes.isEmpty()) {
    currentMode=(currentMode + 1) % metricsModes.size();
    while (!metricsModes.get(currentMode).isAvailable()) {
      currentMode=(currentMode + 1) % metricsModes.size();
    }
    PerformanceMonitor.setEnabled(metricsModes.get(currentMode).isPerformanceManagerMode());
  }
}
