{
  if (data.isArray()) {
    PersistedDataArray dataArray=data.getAsArray();
    if (dataArray.isNumberArray() && dataArray.size() > 3) {
      TFloatList floats=dataArray.getAsFloatArray();
      return new Color(floats.get(0),floats.get(1),floats.get(2),floats.get(3));
    }
  }
  return null;
}
