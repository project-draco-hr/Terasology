{
  return new Predicate<Vector3i>(){
    @Override public boolean apply(    Vector3i input){
      int x=input.getX();
      int y=input.getY();
      int z=input.getZ();
      return (TeraMath.ceilToInt(surface.getWorld(x - 1,z)) == y) && (TeraMath.ceilToInt(surface.getWorld(x + 1,z)) == y) && (TeraMath.ceilToInt(surface.getWorld(x,z - 1)) == y)&& (TeraMath.ceilToInt(surface.getWorld(x,z + 1)) == y);
    }
  }
;
}
