{
  float max=0;
  float maxElement=0;
  for (  UIDisplayElement element : getDisplayElements()) {
    if (element instanceof UIStyle || !element.isVisible()) {
      continue;
    }
    maxElement=element.getPosition().y + element.getSize().y;
    if (maxElement > max) {
      max=maxElement;
    }
  }
  setSize("100%",max + "px");
}
