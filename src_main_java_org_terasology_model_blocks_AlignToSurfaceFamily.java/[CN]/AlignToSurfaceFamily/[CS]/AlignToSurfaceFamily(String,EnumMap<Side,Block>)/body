{
  _name=name;
  for (  Side side : Side.values()) {
    Block block=blocks.get(side);
    if (block != null) {
      _blocks.put(side,block);
      block.withBlockFamily(this);
    }
  }
  if (_blocks.containsKey(Side.TOP)) {
    _archetype=_blocks.get(Side.TOP);
  }
 else {
    _archetype=_blocks.get(Side.FRONT);
  }
}
