{
  try {
    glNewList(_dlClouds,GL_COMPILE);
    glBegin(GL_QUADS);
    int length=_clouds.length;
    for (int x=0; x < length; x++) {
      for (int y=0; y < length; y++) {
        if (!_clouds[x][y]) {
          Primitives.drawCloud(64,32,64,x * 64f - (length / 2 * 64f),0,y * 64f - (length / 2 * 64f));
        }
      }
    }
    glEnd();
    glEndList();
  }
 catch (  Exception ignored) {
  }
}
