{
  if (_time >= 16f && _time <= 24) {
    _daylight=(float)Math.pow(0.8f,(_time - 16f));
  }
 else   if (_time >= 4 && _time <= 12) {
    _daylight=1f - (float)Math.pow(0.8f,(_time - 4f));
  }
 else   if (_time >= 0 && _time < 5) {
    _daylight=(float)Math.pow(0.8f,9);
  }
 else {
    _daylight=1f;
  }
}
