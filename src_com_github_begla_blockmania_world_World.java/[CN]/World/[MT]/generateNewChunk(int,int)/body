{
  Chunk c=_chunkCache.loadOrCreateChunk(x,z);
  if (c == null) {
    return;
  }
  c.generate();
  Chunk[] neighbors=c.loadOrCreateNeighbors();
  for (int i=0; i < neighbors.length; i++) {
    if (neighbors[i] != null) {
      neighbors[i].generate();
    }
  }
  c.updateLight();
  c.writeChunkToDisk();
}
