{
  updateDaytime();
  _player.update();
  _chunkUpdateManager.updateDisplayLists();
  _clouds.update();
  for (FastSet.Record n=_visibleChunks.head(), end=_visibleChunks.tail(); (n=n.getNext()) != end; )   _visibleChunks.valueOf(n).update();
  updateEntities();
  _chunkCache.disposeUnusedChunks();
  updateParticleEmitters();
}
