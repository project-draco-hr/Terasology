{
  updateDaylight();
  updateTicks();
  _skysphere.update();
  _player.update();
  _worldUpdateManager.updateVBOs();
  _clouds.update();
  for (FastList.Node<Chunk> n=_chunksInProximity.head(), end=_chunksInProximity.tail(); (n=n.getNext()) != end; )   n.getValue().update();
  updateParticleEmitters();
}
