{
  FastList<Chunk> result=new FastList<Chunk>();
  FastList<Chunk> chunksInPromity=_chunksInProximity;
  for (FastList.Node<Chunk> n=chunksInPromity.head(), end=chunksInPromity.tail(); (n=n.getNext()) != end; ) {
    Chunk c=n.getValue();
    if (isChunkVisible(c)) {
      c.setVisible(true);
      result.add(c);
      continue;
    }
    c.setVisible(false);
  }
  return result;
}
