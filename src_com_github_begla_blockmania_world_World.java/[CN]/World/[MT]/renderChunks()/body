{
  FastSet<Chunk> visibleChunks=_visibleChunks;
  for (FastSet.Record n=visibleChunks.head(), end=visibleChunks.tail(); (n=n.getNext()) != end; ) {
    visibleChunks.valueOf(n).render(false);
  }
  for (FastSet.Record n=visibleChunks.head(), end=visibleChunks.tail(); (n=n.getNext()) != end; ) {
    visibleChunks.valueOf(n).render(true);
  }
}
