{
synchronized (_updateThread) {
    for (FastSet.Record n=_visibleChunks.head(), end=_visibleChunks.tail(); (n=n.getNext()) != end; ) {
      _visibleChunks.valueOf(n).render(false);
    }
    for (FastSet.Record n=_visibleChunks.head(), end=_visibleChunks.tail(); (n=n.getNext()) != end; ) {
      _visibleChunks.valueOf(n).render(true);
    }
  }
}
