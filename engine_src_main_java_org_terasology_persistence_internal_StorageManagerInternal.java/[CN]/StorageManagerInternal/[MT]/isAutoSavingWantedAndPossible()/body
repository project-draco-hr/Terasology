{
  if (!config.getSystem().isAutoSaveEnabled()) {
    return false;
  }
  if (saveRequested) {
    return true;
  }
  long currentTime=System.currentTimeMillis();
  if (nextAutoSave == null) {
    scheduleNextAutoSave();
    return false;
  }
  if (currentTime >= nextAutoSave) {
    return true;
  }
 else {
    return false;
  }
}
