{
  if (_setting.getSystemBuilder().getActiveViewingDistanceId() == 1)   return getViewingDistanceModerate();
 else   if (_setting.getSystemBuilder().getActiveViewingDistanceId() == 2)   return getViewingDistanceFar();
 else   if (_setting.getSystemBuilder().getActiveViewingDistanceId() == 3)   return getViewingDistanceUltra();
  return getViewingDistanceNear();
}
