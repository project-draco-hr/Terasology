{
  worldSeed=seed;
  BlockManager blockManager=CoreRegistry.get(BlockManager.class);
  world=new WorldBuilder(worldSeed.hashCode()).addProvider(new World2dPreviewProvider()).addProvider(new SeaLevelProvider()).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new PerlinBaseSurfaceProvider()).addProvider(new PerlinRiverProvider()).addProvider(new PerlinOceanProvider()).addProvider(new PerlinHillsAndMountainsProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new FloraProvider()).addProvider(new TreeProvider()).addProvider(new EnsureSpawnableChunkZeroProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).build();
}
