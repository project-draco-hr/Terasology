{
  InjectionHelper.inject(task);
  task.setActor(actor);
  task.setInterpreter(this);
  task.setParent(parent);
  if (parent != null) {
    List<Task> subTasks=startedTasks.get(parent);
    if (subTasks == null) {
      subTasks=Lists.newArrayList();
      startedTasks.put(parent,subTasks);
    }
    subTasks.add(task);
  }
  tasks.addFirst(task);
}
