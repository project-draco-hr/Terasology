{
  int score=Integer.MAX_VALUE;
  regionLock.readLock().lock();
  try {
    for (    CacheRegion region : regions) {
      int dist=distFromRegion(chunk,region.center);
      if (dist < score) {
        score=dist;
      }
    }
    return score;
  }
  finally {
    regionLock.readLock().unlock();
  }
}
