{
switch (mode) {
case SERVER:
    int total=0;
  for (  NetClient client : netClientList) {
    total+=client.getMetrics().getReceivedMessagesSinceLastCall();
  }
return total;
case CLIENT:
if (server != null) {
return server.getMetrics().getReceivedMessagesSinceLastCall();
}
default :
return 0;
}
}
