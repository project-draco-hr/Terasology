{
  if (isMultiline()) {
    Font font=canvas.getCurrentStyle().getFont();
    List<String> lines=TextLineBuilder.getLines(font,text.get(),areaHint.x);
    return font.getSize(lines);
  }
 else {
    Font font=canvas.getCurrentStyle().getFont();
    return new Vector2i(font.getWidth(getText()),font.getLineHeight());
  }
}
