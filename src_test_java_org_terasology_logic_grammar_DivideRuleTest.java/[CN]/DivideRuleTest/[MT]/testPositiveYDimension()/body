{
  Shape reference1=succ1;
  reference1.setDimension(2,(int)heightY1,3);
  Shape reference2=succ2;
  reference2.setDimension(2,(int)heightY2,3);
  reference2.setPosition(0,3,0);
  assertTrue(verticalDivPositiveY.getElements().contains(reference1));
  assertTrue(verticalDivPositiveY.getElements().contains(reference2));
  assertEquals(2,verticalDivPositiveY.getElements().size());
  assertEquals("Wrong translation vector for reference2!",new Vector3i(0,3,0),verticalDivPositiveY.getElements().get(verticalDivPositiveY.getElements().indexOf(reference2)).getMatrix().getTranslation());
  assertEquals("Wrong translation vector for reference1!",new Vector3i(0,0,0),verticalDivPositiveY.getElements().get(verticalDivPositiveY.getElements().indexOf(reference1)).getMatrix().getTranslation());
}
