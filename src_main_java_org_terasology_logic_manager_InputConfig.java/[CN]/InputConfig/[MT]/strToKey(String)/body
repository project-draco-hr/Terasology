{
  if (skey.startsWith("MOUSE")) {
    if (skey == "MOUSELEFT") {
      return 256;
    }
 else     if (skey == "MOUSERIGHT") {
      return 257;
    }
 else     if (skey == "MOUSEMIDDLE") {
      return 258;
    }
 else     if (skey == "MOUSEWHEELUP") {
      return 259;
    }
 else     if (skey == "MOUSEWHEELDOWN") {
      return 260;
    }
 else {
      return 261;
    }
  }
 else {
    return Keyboard.getKeyIndex(skey);
  }
}
