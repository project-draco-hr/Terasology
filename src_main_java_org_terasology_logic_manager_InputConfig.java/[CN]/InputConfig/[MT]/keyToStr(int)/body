{
  if (key < 256) {
    return Keyboard.getKeyName(key);
  }
 else {
    if (key == 256) {
      return "MOUSELEFT";
    }
 else     if (key == 257) {
      return "MOUSERIGHT";
    }
 else     if (key == 258) {
      return "MOUSEMIDDLE";
    }
 else     if (key == 259) {
      return "MOUSEWHEELUP";
    }
 else     if (key == 260) {
      return "MOUSEWHEELDOWN";
    }
 else {
      return "MOUSEMOVE";
    }
  }
}
