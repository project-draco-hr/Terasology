{
  Class<?> clazz=getClass();
  Method[] methods=clazz.getDeclaredMethods();
  List<Method> result=Lists.newArrayList();
  for (  Method method : methods) {
    String currentMethodName=method.getName();
    if (currentMethodName.equals(methodName)) {
      result.add(method);
    }
  }
  return result;
}
