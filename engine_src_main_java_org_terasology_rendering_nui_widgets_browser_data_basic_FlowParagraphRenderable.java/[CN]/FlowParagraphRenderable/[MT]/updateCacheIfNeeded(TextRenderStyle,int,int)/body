{
  if (lineBuilderCache == null || width != lineBuilderCache.width || insetWidth != lineBuilderCache.insetWidth || insetHeight != lineBuilderCache.insetHeight) {
    lineBuilderCache=new LineBuilderCache(width,insetWidth,insetHeight,FlowLineBuilder.getLines(flowParagraphData,defaultStyle,new RenderSpace(){
      @Override public int getWidthForVerticalPosition(      int y){
        if (y >= insetHeight) {
          return width;
        }
 else {
          return width - insetWidth;
        }
      }
    }
));
  }
}
