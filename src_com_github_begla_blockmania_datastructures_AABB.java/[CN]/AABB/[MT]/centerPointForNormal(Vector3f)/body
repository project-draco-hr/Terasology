{
  if (normal.x == 1 && normal.y == 0 && normal.z == 0)   return VectorPool.getVector(_position.x + _dimensions.x,_position.y,_position.z);
  if (normal.x == -1 && normal.y == 0 && normal.z == 0)   return VectorPool.getVector(_position.x - _dimensions.x,_position.y,_position.z);
  if (normal.x == 0 && normal.y == 0 && normal.z == 1)   return VectorPool.getVector(_position.x,_position.y,_position.z + _dimensions.z);
  if (normal.x == 0 && normal.y == 0 && normal.z == -1)   return VectorPool.getVector(_position.x,_position.y,_position.z - _dimensions.z);
  if (normal.x == 0 && normal.y == 1 && normal.z == 0)   return VectorPool.getVector(_position.x,_position.y + _dimensions.y,_position.z);
  if (normal.x == 0 && normal.y == -1 && normal.z == 0)   return VectorPool.getVector(_position.x,_position.y - _dimensions.y,_position.z);
  return VectorPool.getVector();
}
