{
  Vector3f r=VectorPool.getVector(p);
  if (p.x < minX())   r.x=(float)minX();
  if (p.x > maxX())   r.x=(float)maxX();
  if (p.y < minY())   r.y=(float)minY();
  if (p.y > maxY())   r.y=(float)maxY();
  if (p.z < minZ())   r.z=(float)minZ();
  if (p.z > maxZ())   r.z=(float)maxZ();
  return r;
}
