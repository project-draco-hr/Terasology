{
  int chunkId=Chunk.idForPosition(new Vector3f(x,0,z));
  Chunk c=_chunkCache.get(chunkId);
  if (c != null) {
    return c;
  }
  c=loadChunkFromDisk(chunkId);
  if (c == null) {
    c=new Chunk(_parent,new Vector3f(x,0,z));
  }
  _chunkCache.put(c.getChunkId(),c);
  c.setCached(true);
  return c;
}
