{
  ArrayList<WindowListener> listeners=(ArrayList<WindowListener>)windowListeners.clone();
  if (event == EWindowEvent.OPEN) {
    for (    WindowListener listener : listeners) {
      listener.open(this);
    }
  }
 else   if (event == EWindowEvent.CLOSE) {
    for (    WindowListener listener : listeners) {
      listener.close(this);
    }
  }
}
