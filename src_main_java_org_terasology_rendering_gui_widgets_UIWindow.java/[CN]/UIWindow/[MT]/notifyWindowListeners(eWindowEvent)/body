{
  ArrayList<WindowListener> listeners=(ArrayList<WindowListener>)_windowListeners.clone();
  if (event == eWindowEvent.OPEN) {
    for (    WindowListener listener : listeners) {
      listener.open(this);
    }
  }
 else   if (event == eWindowEvent.CLOSE) {
    for (    WindowListener listener : listeners) {
      listener.close(this);
    }
  }
}
