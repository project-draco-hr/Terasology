{
  BlockManager blockManager=CoreRegistry.get(BlockManager.class);
  world=new WorldBuilder(worldSeed.hashCode()).addProvider(new SeaLevelProvider(32)).addProvider(new FlatSurfaceHeightProvider(40)).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new FloraProvider()).addProvider(new TreeProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).build();
}
