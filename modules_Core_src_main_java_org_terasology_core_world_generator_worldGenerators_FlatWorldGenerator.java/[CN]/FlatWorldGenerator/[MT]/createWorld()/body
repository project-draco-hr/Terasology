{
  return new WorldBuilder().addProvider(new SeaLevelProvider(32)).addProvider(new FlatSurfaceHeightProvider(40)).addProvider(new PerlinHumidityProvider()).addProvider(new PerlinSurfaceTemperatureProvider()).addProvider(new BiomeProvider()).addProvider(new SurfaceToDensityProvider()).addProvider(new DefaultFloraProvider()).addProvider(new DefaultTreeProvider()).addRasterizer(new FloraRasterizer()).addRasterizer(new TreeRasterizer()).addRasterizer(new SolidRasterizer()).addPlugins();
}
