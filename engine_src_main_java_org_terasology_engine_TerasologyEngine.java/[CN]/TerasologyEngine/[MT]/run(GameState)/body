{
  try {
    CoreRegistry.putPermanently(GameEngine.class,this);
    if (!initialised) {
      init();
    }
    changeState(initialState);
    running=true;
    Thread.currentThread().setPriority(Thread.MAX_PRIORITY);
    mainLoop();
    cleanup();
  }
 catch (  Throwable t) {
    logger.error("Uncaught exception",t);
    throw t;
  }
}
