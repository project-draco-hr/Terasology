{
  try {
    if (!running) {
      disposed=true;
      initialised=false;
      Iterator<EngineSubsystem> iter=subsystems.descendingIterator();
      while (iter.hasNext()) {
        EngineSubsystem subsystem=iter.next();
        subsystem.dispose();
      }
    }
  }
 catch (  Throwable t) {
    logger.error("Uncaught exception",t);
    throw new RuntimeException("Uncaught exception",t);
  }
}
