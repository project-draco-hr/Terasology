{
  try {
    if (!running) {
      disposed=true;
      initialised=false;
      for (      EngineSubsystem subsystem : getEngineSubsystems()) {
        subsystem.dispose();
      }
    }
  }
 catch (  Throwable t) {
    logger.error("Uncaught exception",t);
    throw new RuntimeException("Uncaught exception",t);
  }
}
