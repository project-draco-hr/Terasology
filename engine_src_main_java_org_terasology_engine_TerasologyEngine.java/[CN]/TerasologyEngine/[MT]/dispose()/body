{
  try {
    disposed=true;
    initialised=false;
    Iterator<EngineSubsystem> iter=subsystems.descendingIterator();
    while (iter.hasNext()) {
      EngineSubsystem subsystem=iter.next();
      subsystem.dispose();
    }
  }
 catch (  RuntimeException e) {
    logger.error("Uncaught exception",e);
    throw e;
  }
}
