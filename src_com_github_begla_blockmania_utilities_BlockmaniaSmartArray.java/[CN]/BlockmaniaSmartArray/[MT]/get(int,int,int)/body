{
  int pos=(x * _lX * _lY) + (y * _lX) + z;
  if (x >= _lX || y >= _lY || z >= _lZ || x < 0 || y < 0 || z < 0)   return -1;
  if (pos < _halfSize) {
    int bArray=_array[pos] & 0xFF;
    return (byte)((bArray & 0x0F) & 0xFF);
  }
  int bArray=_array[pos % _halfSize] & 0xFF;
  return (byte)(bArray >> 4);
}
