{
  try {
    if (modRoot.isDirectory()) {
      File classesDir=new File(modRoot,"classes");
      if (classesDir.exists() && classesDir.isDirectory()) {
        return classesDir.toURI().toURL();
      }
    }
 else {
      return modRoot.toURI().toURL();
    }
  }
 catch (  MalformedURLException e) {
    return null;
  }
  return null;
}
