{
  try {
    if (setter != null) {
      setter.invoke(target,value);
    }
 else {
      field.set(target,value);
    }
  }
 catch (  InvocationTargetException e) {
    logger.error("Exception during setting of {} from {}",field.getName(),target.getClass(),e);
  }
catch (  IllegalAccessException e) {
    logger.error("Exception during setting of {} from {}",field.getName(),target.getClass(),e);
  }
}
