{
  if (TerasologyConstants.ENGINE_MODULE.equals(id)) {
    return engineModule;
  }
  Module result=null;
  for (  Module module : modules.row(UriUtil.normalise(id)).values()) {
    if (result == null || module.getVersion().compareTo(result.getVersion()) > 0) {
      result=module;
    }
  }
  return result;
}
