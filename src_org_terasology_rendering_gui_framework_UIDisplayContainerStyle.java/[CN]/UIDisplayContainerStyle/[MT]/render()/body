{
  if (_backgroundImage != null && _backgroundImage.isVisible()) {
    renderBackgroundImage();
  }
  if (_showBackground && !_showBackgroundImage) {
    renderBackgroundSolid();
  }
  if (_showTexturedBorders) {
    renderTexturedBolders();
  }
 else   if (_showBorders) {
    renderBorderSolid();
  }
}
