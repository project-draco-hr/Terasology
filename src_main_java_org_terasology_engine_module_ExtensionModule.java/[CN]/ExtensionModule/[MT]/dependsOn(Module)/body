{
  for (  DependencyInfo dependencyInfo : moduleInfo.getDependencies()) {
    Module dependency=manager.getLatestModuleVersion(dependencyInfo.getId());
    if (dependency != null) {
      if (module.equals(dependency)) {
        return true;
      }
 else       if (dependency.dependsOn(module)) {
        return true;
      }
    }
  }
  return false;
}
