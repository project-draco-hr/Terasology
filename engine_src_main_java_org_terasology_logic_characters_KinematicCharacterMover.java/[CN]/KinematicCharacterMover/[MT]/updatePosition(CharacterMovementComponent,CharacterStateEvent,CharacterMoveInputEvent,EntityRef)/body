{
switch (state.getMode()) {
case GHOSTING:
    ghost(movementComp,state,input,entity);
  break;
case SWIMMING:
swim(movementComp,state,input,entity);
break;
case WALKING:
walk(movementComp,state,input,entity);
break;
case CLIMBING:
climb(movementComp,state,input,entity);
break;
case NONE:
followToParent(state,entity);
break;
default :
walk(movementComp,state,input,entity);
break;
}
}
