{
  if (_lock.tryLock()) {
    try {
      if (_vertexElements == null || _disposed)       return false;
      for (int i=0; i < _vertexBuffers.length; i++)       generateVBO(i);
      _vertexElements=null;
      _triangles=(_vertexCount[0] + _vertexCount[1] + _vertexCount[2]+ _vertexCount[3]) / 3;
    }
  finally {
      _lock.unlock();
    }
    return true;
  }
  return false;
}
