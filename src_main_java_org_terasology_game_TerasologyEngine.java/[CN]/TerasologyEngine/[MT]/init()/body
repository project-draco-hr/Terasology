{
  if (initialised) {
    return;
  }
  initLogger();
  logger.log(Level.INFO,"Initializing Terasology...");
  logger.log(Level.INFO,TerasologyVersion.getInstance().toString());
  initConfig();
  initNativeLibs();
  initDisplay();
  initOpenGL();
  initOpenAL();
  initControls();
  initManagers();
  updateInputConfig();
  initTimer();
  initSecurity();
  initialised=true;
}
