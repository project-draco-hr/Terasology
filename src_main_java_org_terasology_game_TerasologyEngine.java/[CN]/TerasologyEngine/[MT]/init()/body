{
  if (initialised) {
    return;
  }
  initLogger();
  logger.info("Initializing Terasology...");
  logger.info(TerasologyVersion.getInstance().toString());
  initConfig();
  initNativeLibs();
  initDisplay();
  initOpenGL();
  initOpenAL();
  initControls();
  initTimer();
  initManagers();
  updateInputConfig();
  initSecurity();
  initialised=true;
}
