{
  try {
    textureMap=TextureLoader.getTexture("PNG",new FileInputStream(Chunk.class.getResource("/com/github/begla/blockmania/images/Terrain.png").getPath()),GL_NEAREST);
    textureMap.bind();
  }
 catch (  IOException ex) {
