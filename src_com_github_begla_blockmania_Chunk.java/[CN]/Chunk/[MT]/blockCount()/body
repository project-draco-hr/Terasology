{
  int counter=0;
  for (int x=0; x < (int)Configuration.CHUNK_DIMENSIONS.x; x++) {
    for (int z=0; z < (int)Configuration.CHUNK_DIMENSIONS.z; z++) {
      for (int y=0; y < (int)Configuration.CHUNK_DIMENSIONS.y; y++) {
        if (_blocks[x][y][z] > 0) {
          counter++;
        }
      }
    }
  }
  return counter;
}
