{
  ChunkImpl chunk=getProvider().getChunkForProcessing(getPosition());
  if (chunk == null) {
    return;
  }
  chunk.lock();
  try {
    if (chunk.isDisposed()) {
      return;
    }
    InternalLightProcessor.generateInternalLighting(chunk);
    chunk.deflate();
    getProvider().onChunkIsReady(chunk);
  }
  finally {
    chunk.unlock();
  }
}
