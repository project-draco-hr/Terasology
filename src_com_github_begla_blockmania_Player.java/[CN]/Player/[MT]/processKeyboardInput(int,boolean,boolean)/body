{
switch (key) {
case Keyboard.KEY_E:
    if (state && !repeatEvent) {
      placeBlock(_selectedBlockType);
    }
  break;
case Keyboard.KEY_Q:
if (state && !repeatEvent) {
  removeBlock();
}
break;
case Keyboard.KEY_UP:
if (!repeatEvent && state) {
cycleBlockTypes(1);
}
break;
case Keyboard.KEY_DOWN:
if (!repeatEvent && state) {
cycleBlockTypes(-1);
}
break;
case Keyboard.KEY_SPACE:
if (!repeatEvent && state) {
jump();
}
break;
}
}
