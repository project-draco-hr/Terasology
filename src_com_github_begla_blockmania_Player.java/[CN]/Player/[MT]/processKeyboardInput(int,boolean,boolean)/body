{
switch (key) {
case Keyboard.KEY_E:
    placeBlock(_selectedBlockType);
  break;
case Keyboard.KEY_Q:
removeBlock();
break;
case Keyboard.KEY_UP:
cycleBlockTypes(1);
break;
case Keyboard.KEY_DOWN:
cycleBlockTypes(-1);
break;
case Keyboard.KEY_SPACE:
if (!repeatEvent && state) {
jump();
}
break;
}
}
