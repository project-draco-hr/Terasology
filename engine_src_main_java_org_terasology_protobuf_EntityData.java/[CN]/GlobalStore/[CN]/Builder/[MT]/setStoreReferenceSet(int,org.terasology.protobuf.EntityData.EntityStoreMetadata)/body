{
  if (storeReferenceSetBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureStoreReferenceSetIsMutable();
    storeReferenceSet_.set(index,value);
    onChanged();
  }
 else {
    storeReferenceSetBuilder_.setMessage(index,value);
  }
  return this;
}
