{
  if (window == null) {
    logger.warn("Can't open window: null");
  }
 else {
    if (!renderer.getDisplayElements().contains(window)) {
      addWindow(window);
    }
    logger.debug("Open and focus window by reference with ID \"{}\"",window.getId());
    renderer.setWindowFocus(window);
  }
  return window;
}
