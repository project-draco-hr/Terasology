{
  UIDisplayWindow window=getWindowById(windowId);
  if (window == null) {
    return false;
  }
  if (_focusedWindow != null && _focusedWindow.isModal() && _focusedWindow.isVisible()) {
    if (window == _focusedWindow) {
      window.setVisible(false);
      return true;
    }
  }
 else {
    setFocusedWindow(window);
    return true;
  }
  return false;
}
