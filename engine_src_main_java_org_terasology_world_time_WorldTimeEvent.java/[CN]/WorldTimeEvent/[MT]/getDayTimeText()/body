{
  if (isDusk()) {
    return "dusk";
  }
  if (isMorning()) {
    return "morning";
  }
  if (isMidday()) {
    return "midday";
  }
  if (isAfternoon()) {
    return "afternoon";
  }
  if (isDawn()) {
    return "dawn";
  }
  if (isNight()) {
    return "night";
  }
  if (isMidnight()) {
    return "mignight";
  }
  return "UNDEFINED";
}
