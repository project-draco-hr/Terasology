{
  if (netTick) {
    NetData.NetMessage.Builder message=NetData.NetMessage.newBuilder();
    message.setTime(timer.getTimeInMs());
    sendRemovedEntities(message);
    sendInitialEntities(message);
    sendDirtyEntities(message);
    sendEvents(message);
    send(message.build());
  }
  processReceivedMessages();
}
