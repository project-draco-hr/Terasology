{
  try {
    String fullPath="/" + fname;
    URL rsc=CrashReporter.class.getResource(fullPath);
    if (rsc == null) {
      throw new FileNotFoundException(fullPath);
    }
    BufferedImage image=ImageIO.read(rsc);
    return new ImageIcon(image);
  }
 catch (  IOException e) {
    e.printStackTrace(System.err);
    return null;
  }
}
