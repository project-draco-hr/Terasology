{
  if (Desktop.isDesktopSupported()) {
    Desktop desktop=Desktop.getDesktop();
    if (desktop.isSupported(Desktop.Action.BROWSE)) {
      try {
        desktop.browse(new URI(url));
      }
 catch (      IOException|URISyntaxException e) {
        e.printStackTrace(System.err);
      }
    }
  }
}
