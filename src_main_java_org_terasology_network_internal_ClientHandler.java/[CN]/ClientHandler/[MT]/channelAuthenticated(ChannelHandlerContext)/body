{
  this.server=new Server(networkSystem,ctx.getChannel());
  networkSystem.setServer(server);
  Config config=CoreRegistry.get(Config.class);
  ctx.getChannel().write(NetMessage.newBuilder().setClientConnect(ClientConnectMessage.newBuilder().setName(config.getPlayer().getName()).setViewDistanceLevel(config.getRendering().getViewDistance().getIndex())).build());
}
