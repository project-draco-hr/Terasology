{
  if (file != null) {
    try {
      file.close();
    }
 catch (    IOException e) {
      logger.warn("Failed to close streaming sound: {}",getURI(),e);
    }
  }
  try {
    file=new OggReader(audioSource.openStream());
  }
 catch (  IOException e) {
    logger.error("Failed to load streaming sound: {}",getURI(),e);
  }
}
