{
  blocksById.clear();
  blocksByPrefabName.clear();
  blocksByUri.clear();
  familyByUri.clear();
  idByUri.clear();
  partiallyRegisteredFamilies.clear();
  shapelessBlockDefinition.clear();
  nextId=1;
  blockLoader=new BlockLoader();
  categoryLookup.clear();
