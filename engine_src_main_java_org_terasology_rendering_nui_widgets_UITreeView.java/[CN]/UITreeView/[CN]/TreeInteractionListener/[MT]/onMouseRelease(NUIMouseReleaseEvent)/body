{
  if (mouseOverIndex.get() != null && selectedIndex.get() != null && !mouseOverIndex.get().equals(selectedIndex.get())) {
    logger.info("Dragging " + model.get().getElement(selectedIndex.get()).getValue() + " onto "+ model.get().getElement(mouseOverIndex.get()).getValue()+ "...");
  }
  mouseOverIndex.set(null);
  selectedIndex.set(null);
}
