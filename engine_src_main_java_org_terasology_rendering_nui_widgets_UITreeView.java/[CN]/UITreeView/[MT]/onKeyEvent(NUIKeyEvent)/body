{
  if (event.isDown()) {
    int id=event.getKey().getId();
    if (id == Keyboard.KeyId.UP || id == Keyboard.KeyId.DOWN) {
      moveSelectedElement(id);
      return true;
    }
 else     if (id == Keyboard.KeyId.DELETE) {
      removeSelectedElement();
      return true;
    }
 else {
      return false;
    }
  }
  return false;
}
