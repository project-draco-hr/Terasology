{
  entityManager.clear();
  FileInputStream in=new FileInputStream(file);
  EntityData.World world=null;
  try {
    world=format.load(in);
  }
  finally {
    try {
      in.close();
    }
 catch (    IOException e) {
      logger.log(Level.SEVERE,"Failed to close file",e);
    }
  }
  if (world != null) {
    persisterHelper.deserializeWorld(world);
  }
}
