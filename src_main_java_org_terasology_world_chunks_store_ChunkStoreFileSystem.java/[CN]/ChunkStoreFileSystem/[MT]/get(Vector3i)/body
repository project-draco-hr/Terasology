{
  File f=new File(worldPath,getFileNameFor(id));
  if (!f.exists())   return null;
  try {
    FileInputStream fileIn=new FileInputStream(f);
    ObjectInputStream in=new ObjectInputStream(fileIn);
    Chunk result=(Chunk)in.readObject();
    in.close();
    fileIn.close();
    return result;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
