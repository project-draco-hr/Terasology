{
  this.server=new Server(networkSystem,e.getChannel());
  networkSystem.setServer(server);
  Config config=CoreRegistry.get(Config.class);
  e.getChannel().write(NetMessage.newBuilder().setClientConnect(ClientConnectMessage.newBuilder().setName(config.getPlayer().getName()).setViewDistanceLevel(config.getRendering().getActiveViewDistanceMode())).build());
}
