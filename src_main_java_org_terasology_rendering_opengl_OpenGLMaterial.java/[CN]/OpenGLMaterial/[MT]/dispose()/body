{
  Util.checkGLError();
  if (shaderProgram != 0) {
    Material activeMaterial=ShaderManager.getInstance().getActiveMaterial();
    if (activeMaterial == this) {
      ShaderManager.getInstance().enableDefault();
    }
    GL20.glDeleteProgram(shaderProgram);
    shaderProgram=0;
  }
  Util.checkGLError();
}
