{
  color=color.trim().toLowerCase();
  int r=0;
  int g=0;
  int b=0;
  if (color.matches("^#[a-f0-9]{1,6}$")) {
    color=color.replace("#","");
    int sum=Integer.parseInt(color,16);
    r=(sum & 0x00FF0000) >> 16;
    g=(sum & 0x0000FF00) >> 8;
    b=sum & 0x000000FF;
  }
  setColor(r,g,b,a);
}
