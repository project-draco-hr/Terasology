{
  Chunk chunk=new ChunkImpl(new Vector3i());
  chunk.setBlock(new Vector3i(0,0,0),solidBlock);
  Chunk[] chunks=new Chunk[]{new ChunkImpl(new Vector3i(-1,0,-1)),new ChunkImpl(new Vector3i(0,0,-1)),new ChunkImpl(new Vector3i(1,0,-1)),new ChunkImpl(new Vector3i(-1,0,0)),chunk,new ChunkImpl(new Vector3i(1,0,0)),new ChunkImpl(new Vector3i(-1,0,1)),new ChunkImpl(new Vector3i(0,0,1)),new ChunkImpl(new Vector3i(1,0,1))};
  ChunkViewCore chunkView=new ChunkViewCoreImpl(chunks,Region3i.createFromCenterExtents(new Vector3i(0,0,0),new Vector3i(1,0,1)),new Vector3i(1,0,1),airBlock);
  assertEquals(solidBlock,chunkView.getBlock(0,0,0));
}
