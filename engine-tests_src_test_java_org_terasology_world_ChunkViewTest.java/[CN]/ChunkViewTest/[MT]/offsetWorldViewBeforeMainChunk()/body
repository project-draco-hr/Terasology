{
  Chunk chunk=new ChunkImpl(new Vector3i());
  chunk.setBlock(new Vector3i(15,0,15),solidBlock);
  Chunk[] chunks=new Chunk[]{chunk,new ChunkImpl(new Vector3i(0,0,-1)),new ChunkImpl(new Vector3i(1,0,-1)),new ChunkImpl(new Vector3i(-1,0,0)),new ChunkImpl(new Vector3i(0,0,0)),new ChunkImpl(new Vector3i(1,0,0)),new ChunkImpl(new Vector3i(-1,0,1)),new ChunkImpl(new Vector3i(0,0,1)),new ChunkImpl(new Vector3i(1,0,1))};
  ChunkViewCore chunkView=new ChunkViewCoreImpl(chunks,Region3i.createFromCenterExtents(new Vector3i(0,0,0),new Vector3i(1,0,1)),new Vector3i(1,0,1));
  assertEquals(solidBlock,chunkView.getBlock(-1,0,-1));
}
