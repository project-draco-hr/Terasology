{
  if (System.getProperty("os.name").equals("Mac OS X"))   addLibraryPath("natives/macosx");
 else   if (System.getProperty("os.name").equals("Linux"))   addLibraryPath("natives/linux");
 else {
    addLibraryPath("natives/windows");
    if (System.getProperty("os.arch").contains("64")) {
      System.loadLibrary("OpenAL64");
      _logger.log(Level.INFO,"Initializing openAl 64");
    }
 else {
      System.loadLibrary("OpenAL32");
      _logger.log(Level.INFO,"Initializing openAl 32");
    }
  }
}
